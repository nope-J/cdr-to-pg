<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cdr.mapper.CdrHandleRecordMapper" >
    <resultMap id="BaseResultMap" type="com.cdr.model.CdrHandleRecord" >

        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="file_key" property="fileKey" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <select id="selectLatestHandleRecord" resultMap="BaseResultMap">
        select id,file_key,create_time from cdr_handle_record where id=(select max(id) from cdr_handle_record)
    </select>
</mapper>